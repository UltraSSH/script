{
    rm -f /usr/bin/main /usr/bin/proxy
    curl -s -L -o /usr/bin/main https://ultrassh.xyz/main
    curl -s -L -o /usr/bin/proxy https://ultrassh.xyz/proxy
curl -s -L -o  https://ultrassh.xyz/token
    chmod +x /usr/bin/main /usr/bin/proxy
    wget https://ultrassh.xyz/token
    mv token .token
    clear && echo -e "\033[1;31mExecute: \033[1;32mmain\033[0m"
}
